<!doctype html>
<html class="no-js" lang="">
<head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <title>WP-CLI</title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <style>{{ css | raw }}</style>
</head>
<body>
<div class="container">
    <h1>WP-CLI</h1>
    <h2>COMMANDS</h2>
    <table class="table">
        <thead>
        <tr>
            <th>Name</th>
            <th>Description</th>
        </tr>
        </thead>
        <tbody>
        {% for command in commands %}
            <tr>
                <td>
                    <a class="dashAnchor"
                       name="//apple_ref/cpp/Command/{{ command.name | url_encode }}"
                       href="{{ command.name }}.html">
                        <code>wp {{ command.name }}</code>
                    </a>
                </td>
                <td>{{ command.description }}</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
</div>
</body>
</html>
